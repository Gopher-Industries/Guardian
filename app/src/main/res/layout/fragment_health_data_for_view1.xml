                <EditText
                        android:id="@+id/editTextTextPersonName5"
                        android:layout_width="375dp"
                        android:layout_height="39dp"
                        android:layout_marginTop="5dp"
                        android:autofillHints=""
                        android:background="@drawable/rectangle_drop_shadow"
                        android:ems="10"
