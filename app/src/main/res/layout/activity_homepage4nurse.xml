<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="deakin.gopher.guardian.view.general.Homepage4nurse">

    <Button
        android:id="@+id/settingsButton_nurse"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="88dp"
        android:background="@drawable/homepage_button_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <View
        android:id="@+id/settingsIcon_nurse"
        android:layout_width="53dp"
        android:layout_height="43dp"
        android:layout_marginStart="256dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/menu_settings"
        android:elevation="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <TextView
        android:id="@+id/settingsTxt_nurse"
        android:layout_width="102dp"
        android:layout_height="26dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="168dp"
        android:elevation="4dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/settings"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <Button
        android:id="@+id/tasksButton_nurse"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="88dp"
        android:background="@drawable/homepage_button_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <TextView
        android:id="@+id/tasksTxt_nurse"
        android:layout_width="88dp"
        android:layout_height="28dp"
        android:layout_marginTop="168dp"
        android:elevation="4dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/tasks_label"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="@+id/tasksButton_nurse"
        app:layout_constraintStart_toStartOf="@+id/tasksButton_nurse"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <View
        android:id="@+id/patientListIcon_nurse"
        android:layout_width="42dp"
        android:layout_height="44dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/menu_button"
        android:elevation="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <Button
        android:id="@+id/sighOutButton_nurse"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/homepage_button_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <TextView
        android:id="@+id/sighOutTxt_nurse"
        android:layout_width="82dp"
        android:layout_height="22dp"
        android:layout_marginStart="172dp"
        android:layout_marginTop="320dp"
        android:elevation="4dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/sign_out"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <View
        android:id="@+id/sighOutIcon_nurse"
        android:layout_width="53dp"
        android:layout_height="43dp"
        android:layout_marginStart="188dp"
        android:layout_marginTop="268dp"
        android:background="@drawable/menu_signout"
        android:elevation="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalDiaganosticsHeaderCardView" />

    <androidx.cardview.widget.CardView
        android:id="@+id/medicalDiaganosticsHeaderCardView"
        android:layout_width="0dp"
        android:layout_height="220dp"
        android:layout_marginTop="-40dp"
        app:cardBackgroundColor="@color/TG_blue"
        app:cardCornerRadius="40dp"
        app:cardElevation="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/medicalDiagnosticsTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:fontFamily="@font/poppins_bold"
                android:text="@string/welcome_nurse"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/medicalDiagnosticsEditImageView" />

            <ImageView
                android:id="@+id/medicalDiagnosticsEditImageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="54dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/medicalDiagnosticsTitleTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/profile_avatar_women" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>