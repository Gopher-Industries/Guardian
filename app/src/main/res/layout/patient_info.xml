<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="deakin.gopher.guardian.view.general.PatientProfileAddActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/patient_list_backgroud"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".view.general.TasksListActivity">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/Profile_Avatar"
            android:translationZ="12dp"
            android:visibility="visible"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="198dp"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/patient_8"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="157dp" />


        <ImageView
            android:id="@+id/patient_list_menu_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="51dp"
            android:layout_marginTop="56dp"
            android:contentDescription="TODO"
            app:layout_constraintEnd_toStartOf="@+id/patient_list_company_icon"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/menu_button" />

        <ImageView
            android:id="@+id/patient_list_company_icon"
            android:layout_width="67dp"
            android:layout_height="60dp"
            android:layout_marginTop="44dp"
            android:contentDescription="TODO"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/gopher_logo" />

        <ImageView
            android:id="@+id/patient_list_head_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="72dp"
            android:layout_marginTop="44dp"
            android:contentDescription="TODO"
            app:layout_constraintStart_toEndOf="@+id/patient_list_company_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/image" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="390dp"
            android:layout_height="105dp"
            android:src="@drawable/Patient_2"
            tools:layout_editor_absoluteX="12dp"
            tools:layout_editor_absoluteY="395dp" />


        <ImageView
            android:layout_width="462dp"
            android:layout_height="318dp"
            android:src="@drawable/patient_medication"
            tools:layout_editor_absoluteX="-26dp"
            tools:layout_editor_absoluteY="500dp" />

        <ImageView
            android:id="@+id/imageView61"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginStart="188dp"
            android:contentDescription="TODO"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/three_point"
            tools:layout_editor_absoluteY="808dp" />

        <androidx.constraintlayout.helper.widget.Layer
            android:id="@+id/layer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Diagnosis:"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintHorizontal_bias="0.068"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            tools:layout_editor_absoluteY="424dp"
            android:textStyle="bold"
            android:textSize="18dp"
            android:textColor="@color/black"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.drawerlayout.widget.DrawerLayout>